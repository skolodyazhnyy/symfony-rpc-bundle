{
    "name": "seven/symfony-rpc-bundle",
    "description": "This Symfony bundle provide easy way to create XMLRPC server",
    "keywords": ["Symfony", "XMLRPC", "Server", "XML-RPC", "RPC", "Bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Kolodyazhnyy",
            "email": "sergey.kolodyazhnyy@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-0": { "Seven\\RpcBundle": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "target-dir": "Seven/RpcBundle"
}
